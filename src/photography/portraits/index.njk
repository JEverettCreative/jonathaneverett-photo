{# ===== COSPLAY ===== #}
<section class="section section-dark">
  <div class="container">
    <div class="section-title fade-in">
      <h2>Cosplay</h2>
      <div class="gold-divider"></div>
    </div>
    <div class="gallery-grid stagger-children">
      {% set cosplayImages = [
        { src: "05212_uncropped" },
        { src: "05755_social" },
        { src: "fiery_claws" },
        { src: "retouched_03319" },
        { src: "green_lantern" },
        { src: "shield_and_sword" },
        { src: "stella_promo" }
      ] %}
      {% for img in cosplayImages %}
      <div class="gallery-item">
        <picture>
          <source srcset="/images/cosplay/{{ img.src }}-800w.webp 800w, /images/cosplay/{{ img.src }}-1200w.webp 1200w, /images/cosplay/{{ img.src }}-1920w.webp 1920w" sizes="(max-width: 600px) 100vw, (max-width: 1000px) 50vw, 33vw" type="image/webp">
          <img src="/images/cosplay/{{ img.src }}-1200w.webp" alt="Cosplay photography" loading="lazy">
        </picture>
      </div>
      {% endfor %}
    </div>
  </div>
</section>

